    "def cipher(text):\n",
    "    result = []\n",
    "    for w in text:\n",
    "        if w.islower():\n",
    "            result.append(chr(219 - ord(w)))\n",
    "        else:\n",
    "            result.append(w)\n",
    "                          \n",
    "    return \"\".join(result)\n",
    "\n",
    "print(cipher(\"test\"))"
   ]
